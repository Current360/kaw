<% @selected.each do |selected| %>
<figure class="bio-pic">
  <%# image_tag selected.bio_pic.large_bio_pic %>
</figure>
<h2 class="bio-name">
  <%= selected.first_name %>
  <span class="last-name"><%= selected.last_name %></span>
</h2>
<ul class="titles">
  <li><%= selected.funny_title %></li>
  <li><%= selected.real_title %></li>
</ul>
<p><%= selected.overview %></p>
<ul class="questions">
  <li><span class="question"><%= selected.question1 %></span><%= selected.answer1 %></li>
  <li><span class="question"><%= selected.question2 %></span><%= selected.answer2 %></li>
  <li><span class="question"><%= selected.question3 %></span><%= selected.answer3 %></li>
  <li><span class="question"><%= selected.question4 %></span><%= selected.answer4 %></li>
  <li><span class="question"><%= selected.question5 %></span><%= selected.answer5 %></li>
</ul>
<% end %>
